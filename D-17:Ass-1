//Find Lowest among n numbers.
#include<stdio.h>
int main(){
    int i,n,h,element;
    scanf("%d",&n);
    i=1;
    while (i<=n)
    {
        scanf("%d",&element);
        if(i==1)h=element;
        if(element<h)
        h=element;
        
        i++;
    }
    printf("The lowest is %d",h);
    return 0;
    
}
