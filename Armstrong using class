#Armstrong number using class
class check:
    def armstrong(Self,n):
        m=0
        temp=n
        while n!=0:
            r=n%10
            m=m+(r*r*r)
            n=n//10
        if m==temp:
            print("Armstrong Number")
        else:
            print("Not a armstrong number")

n=int(input())
obj=check()
obj.armstrong(n)
