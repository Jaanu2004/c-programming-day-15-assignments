//A c program to check Whether the given number is Armstrong number or not
#include<stdio.h>
int main(){
	int n,r,sum=0,temp;
	scanf("%d",&n);
	temp=n;
	while(n!=0){
		r=n%10;
		sum=sum+(r*r*r);
		n=n/10;
		}
	if(temp==sum)
		printf("It is Armstrong Number");
	else 
		printf("It is not a Armstrong Number");
        return 0;
}
