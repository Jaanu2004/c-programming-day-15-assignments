//A C program to print difference between even sum and odd sum of n numbers

#include<stdio.h>
int main(){
	int i,n,even=0,odd=0;
	i=1;
	scanf("%d",&n);
	while(i<=n){
	   
	    if(i%2==0)
		even=even+i;
		else
		odd=odd+i;
		i++;
	}
	printf("Even sum=%d\n",even);
	printf("Odd sum=%d\n",odd);
    printf("Difference =%d",odd-even);
	return 0;
}
